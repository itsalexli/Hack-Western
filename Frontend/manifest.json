{
  "manifest_version": 3,
  "name": "simplify-overlay",
  "version": "1.0",
  "description": "Adds an overlay with a toggle button to the bottom right corner of every page",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["overlay.css"],
      "run_at": "document_idle"

    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["test.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
